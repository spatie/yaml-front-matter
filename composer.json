{
    "name": "spatie/yaml-front-matter",
    "description": "A to the point yaml front matter parser",
    "keywords": [
        "spatie",
        "yaml",
        "front matter",
        "jekyll"
    ],
    "homepage": "https://github.com/sebastiandedeyne/yaml-front-matter",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\YamlFrontMatter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\YamlFrontMatter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
